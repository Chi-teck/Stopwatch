{
    "name": "chi-teck/stopwatch",
    "description": "A tool for benchmarking and profiling PHP code.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ChiTeck\\Stopwatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ChiTeck\\Stopwatch\\": "tests/src"
        }
    },
    "authors": [
        {
            "name": "me"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "friendsofphp/php-cs-fixer": "^3.62",
        "phpunit/phpunit": "^11.3"
    }
}
